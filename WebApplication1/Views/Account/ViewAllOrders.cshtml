@model List<WebApplication1.Models.Order>

<h2>Comenzi</h2>
<table class="table table-striped">
    <thead>
    <tr>
        <th>Order ID</th>
        <th>User ID</th>
        <th>SuperBox ID</th>
        <th>Is Urgent</th>
        <th>Status</th>
        <th>Relevant Info</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.OrderId</td>
            <td>@order.UserId</td>
            <td>@order.SuperBoxId</td>
            <td>@(order.IsUrgent ? "Yes" : "No")</td>
            <td>@order.Status</td>
            <td>@order.RelevantInfo</td>
        </tr>
    }
    </tbody>
</table>